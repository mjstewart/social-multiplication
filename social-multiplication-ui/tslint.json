{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "jsx-boolean-value": false,
    "interface-name": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false
  }
}